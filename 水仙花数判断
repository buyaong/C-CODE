int main()
{
	int n, ge, shi, bai, n1;
	scanf("%d", &n);
	bai = n / 100;
	n1 = n % 100;
	shi = n1 / 10;
	ge = n1 % 10;
	if (n >= 100 && n <= 999)
	{
		if (n==(ge* ge* ge + shi * shi * shi + bai * bai * bai))
			printf("yes");
		else
			printf("no");
	}
	else
		printf("Invalid Value");

	return 0;
}
