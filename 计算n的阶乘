int main()
{
	int n = 0;
	scanf("%d", &n);
	int i = 0;
	int det = 1;
	for (i = 1; i <= n; i++)
	{
		det = i * det;
	}
	printf("%d\n", det);
	return 0;
}

int main()
{
	int n = 0;
	scanf("%d", &n);
	int i = 1;
	int det = 1;
	while (i <= n)
	{
		det = det * i;
		i++;
	}
	printf("%d\n", det);
	return 0;
}
